			"states" : {
				"default" : {
          "animations" : [
            { "base_pose": "v.roll < 5" },
            { "base_pose_sus": "v.roll >= 5" }
          ],
					"transitions" : [
            { "move" : "q.ground_speed > 0.0 && q.is_on_ground && !q.is_riding" },
