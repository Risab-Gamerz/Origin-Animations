          "on_entry": [ "v.roll = math.random_integer(1, 4);" ],
          "animations": [
            { "dead1": "v.roll == 1" },
            { "dead2": "v.roll == 2" },
            { "dead3": "v.roll == 3" },
            { "dead4": "v.roll == 4" }
          ]
        }
			}
    },
