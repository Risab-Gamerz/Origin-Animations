            { "dead3": "v.roll == 3" },
            { "dead4": "v.roll == 4" }
          ]
        }
			}
    },
    "controller.animation.wolf.root" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
          "animations" : [
            { "base_pose": "v.roll < 5" },
            { "base_pose_sus": "v.roll >= 5" }
          ],
					"transitions" : [
            { "move_notarget" : "q.ground_speed > 0.0 && q.is_on_ground && !q.is_riding && !q.has_target" },
            { "move_target" : "q.ground_speed > 0.0 && q.is_on_ground && !q.is_riding && q.has_target" },
            { "sit_trans": "q.is_sitting" },
            { "shake": "q.is_shaking_wetness" },
            { "interested": "q.is_interested" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.4,
					"blend_via_shortest_path": true
        },
        "idle": {
          "animations" : [
            { "idle1": "v.roll == 1" },
            { "idle2": "v.roll == 2" },
            { "idle3": "v.roll == 3" }
          ],
					"transitions" : [
            { "default" : "q.has_target || q.is_in_contact_with_water || q.any_animation_finished" },
            { "move_notarget" : "q.ground_speed >= 0.1 && !q.has_target" },
            { "sit_trans": "q.is_sitting" },
            { "shake": "q.is_shaking_wetness" },
            { "interested": "q.is_interested" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "move_notarget": {
          "on_entry": [ "v.roll = math.random_integer(1, 5);" ],
          "animations": [
            "move_upper_body",
            "move_lower_body_notarget"
          ],
          "transitions": [
            { "default": "(q.ground_speed == 0.0 && v.roll > 3) || (q.ground_speed == 0.0 && v.roll <= 3)" },
            { "move_target": "q.has_target" },
            { "idle": "!q.has_target && q.ground_speed == 0.0 && v.roll <= 3" },
            { "sit_trans": "q.is_sitting" },
            { "shake": "q.is_shaking_wetness" },
            { "interested": "q.ground_speed == 0.0 && q.is_interested" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.2,
					"blend_via_shortest_path": true
        },
        "move_target": {
          "on_entry": [ "v.roll = math.random_integer(1, 5);" ],
          "animations": [
            "move_upper_body",
            "move_lower_body_target_front",
            "move_lower_body_target_back"
          ],
          "transitions": [
            { "default": "(q.ground_speed == 0.0 && v.roll > 3) || (q.ground_speed == 0.0 && v.roll <= 3 && q.has_target)" },
            { "idle": "!q.has_target && q.ground_speed == 0.0 && v.roll <= 3" },
            { "attack": "v.attack_time" },
            { "sit_trans": "q.is_sitting" },
            { "shake": "q.is_shaking_wetness" },
            { "interested": "q.is_interested" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "sit_trans": {
          "animations": [ "sit_trans" ],
          "transitions": [
            { "default": "!q.is_sitting" },
            { "sit": "q.any_animation_finished" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "shake": {
          "animations": [ "shake" ],
          "transitions": [
            { "default": "q.any_animation_finished || !q.is_shaking_wetness" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "interested": {
          "animations": [ "interested" ],
          "transitions": [
            { "default": "q.any_animation_finished || !q.is_interested" },
            { "move_notarget" : "q.ground_speed > 0.0 && q.is_on_ground && !q.is_riding && !q.has_target" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "sit": {
          "animations": [ "sit" ],
          "transitions": [
            { "default": "!q.is_sitting" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "attack": {
          "on_entry": [ "v.delay = q.life_time;" ],
          "animations": [
            "move_lower_body_target_front",
            "move_lower_body_target_back",
            "attack"
          ],
          "transitions": [
            { "move_target": "!v.attack_time && q.life_time - v.delay >= 0.5" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "dead": {
          "on_entry": [ "v.roll = math.random_integer(1, 2);" ],
          "animations": [
            { "dead1": "v.roll == 1" },
            { "dead2": "v.roll == 2" }
          ]
        }
			}
    },
