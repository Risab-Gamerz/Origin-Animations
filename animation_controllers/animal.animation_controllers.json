          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "attack": {
          "on_entry": [ "v.delay = q.life_time;" ],
          "animations": [
            "move_lower_body_target_back",
            "attack"
          ],
          "transitions": [
            { "move": "!v.attack_time && q.life_time - v.delay >= 0.5" },
            { "dead": "!q.is_alive" }
          ],
          "blend_transition": 0.1,
					"blend_via_shortest_path": true
        },
        "dead": {
          "on_entry": [ "v.roll = math.random_integer(1, 2);" ],
          "animations": [
            { "dead1": "v.roll == 1" },
            { "dead2": "v.roll == 2" }
          ]
        }
			}
    }
  }
}
