{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:enderman",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "enderman",
        "invisible": "enderman_invisible"
      },
      "textures": {
        "default": "textures/entity/enderman/enderman"
      },
      "geometry": {
        "default": "geometry.enderman.v1.8"
      },
      "scripts": {
        "scale": "q.life_time < 0.2 ? q.life_time * 5 : 1",
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.tcos1 = (Math.cos(query.modified_distance_moved * 76.34) * query.modified_move_speed) * 57.3;"
        ],
        "animate": [
          "root",
          "teleport",
          "carry",
          { "look_at_target": "query.is_alive" },
          { "dead": "!query.is_alive" }
        ],
        "initialize": [
          "v.roll = math.random_integer(1, 5);",
          "v.attack_roll = 0;",
          "v.posx = query.position(0);",
          "v.posz = query.position(2);"
        ]
      },
      "animations": {
        "root": "controller.animation.enderman.root",
        "carry": "controller.animation.enderman.carrying",
        "teleport": "controller.animation.enderman.teleporting",
        "warp": "animation.enderman.warp",
        "carrying": "animation.enderman.carrying",
        "look_at_target": "animation.common.look_at_target",
        "base_pose": "animation.enderman.base_pose",
        "riding.legs": "animation.humanoid.riding.legs",
        "idle1": "animation.enderman.idle1",
        "idle2": "animation.enderman.idle2",
        "idle3": "animation.enderman.idle3",
        "move_upper_body": "animation.enderman.move_upper_body",
        "move_upper_body_target1": "animation.enderman.move_upper_body_target1",
        "move_upper_body_target2": "animation.enderman.move_upper_body_target2",
        "move_upper_body_target3": "animation.enderman.move_upper_body_target3",
        "move_lower_body": "animation.enderman.move_lower_body",
        "attack1": "animation.enderman.attack1",
        "attack2": "animation.enderman.attack2",
        "attack3": "animation.enderman.attack3",
        "attack4": "animation.enderman.attack4",
        "attack5": "animation.enderman.attack5",
        "dead": "animation.custom_death",
        "dead1": "animation.enderman.dead1",
        "dead2": "animation.enderman.dead2",
        "dead3": "animation.enderman.dead3",
        "dead4": "animation.enderman.dead4"
      },
      "render_controllers": [ "controller.render.enderman" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 7
      }
    }
  }
}