{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:ocelot",
      "min_engine_version": "1.8.0",
      "materials": { "default": "ocelot" },
      "textures": {
        "black": "textures/entity/cat/blackcat",
        "red": "textures/entity/cat/red",
        "siamese": "textures/entity/cat/siamese",
        "wild": "textures/entity/cat/ocelot"
      },
      "geometry": {
        "default": "geometry.ocelot.v1.8"
      },
      "scripts": {
        "scale": "q.life_time < 0.2 ? q.life_time * 5 : 1",
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.tcos1 = (Math.cos((query.modified_distance_moved - 5) * 76.34) * query.modified_move_speed) * 57.3;",
          "variable.tcos2 = (Math.cos(query.modified_distance_moved * 14.31375) * query.modified_move_speed) * 57.3;",
          "variable.legrightfront = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.legleftfront = (Math.cos((query.modified_distance_moved - 1.5) * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.legrightback = (Math.cos((query.modified_distance_moved - 3) * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.legleftback = (Math.cos((query.modified_distance_moved - 4.5) * 38.17) * query.modified_move_speed) * 57.3;"
        ],
        "animate": [
          "root",
          { "look_at_target": "query.is_alive" },
          { "baby_transform": "query.is_baby" },
          { "dead": "!query.is_alive" }
        ],
        "initialize": [
          "v.roll = math.random_integer(1, 5);"
        ]
      },
      "animations": {
        "root": "controller.animation.cat.root",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.ocelot.baby_transform",
        "base_pose": "animation.cat.base_pose",
        "base_pose_sus": "animation.cat.base_pose_sus",
        "idle1": "animation.cat.idle1",
        "idle2": "animation.cat.idle2",
        "idle3": "animation.cat.idle3",
        "move_upper_body": "animation.cat.move_upper_body",
        "move_lower_body_notarget": "animation.cat.move_lower_body_notarget",
        "move_lower_body_target_front": "animation.cat.move_lower_body_target_front",
        "move_lower_body_target_back": "animation.cat.move_lower_body_target_back",
        "lie_down": "animation.cat.lie_down",
        "sneak": "animation.cat.sneak",
        "attack": "animation.cat.attack",
        "sit_trans": "animation.cat.sit_trans",
        "sit": "animation.cat.sit",
        "dead": "animation.custom_death",
        "dead1": "animation.cat.dead1",
        "dead2": "animation.cat.dead2"
      },
      "render_controllers": [ "controller.render.ocelot" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 16
      }
    }
  }
}
