{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:vex",
      "min_engine_version": "1.8.0",
      "materials": { "default": "vex" },
      "textures": {
        "default": "textures/entity/vex/vex",
        "charging": "textures/entity/vex/vex_charging"
      },
      "geometry": {
        "default": "geometry.vex.v1.8"
      },
      "scripts": {
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed) * 57.3;",
          "variable.tcos1 = (Math.cos(query.modified_distance_moved * 76.34) * query.modified_move_speed) * 57.3;"
        ],
        "animate": [
          "root",
          { "look_at_target": "query.is_alive" },
          { "dead": "!query.is_alive" }
        ],
        "initialize": [
          "v.roll = math.random_integer(1, 5);",
          "v.attack_roll = math.random_integer(1, 3);"
        ],
        "scale": "q.life_time < 0.2 ? q.life_time * 5 : 1"
      },
      "animations": {
        "root": "controller.animation.vex.root",
        "look_at_target": "animation.common.look_at_target",
        "humanoid_baby_big_head": "controller.animation.humanoid.baby_big_head",
        "humanoid_big_head": "animation.humanoid.big_head",
        "base_pose": "animation.vex.base_pose",
        "riding.legs": "animation.humanoid.riding.legs",
        "idle1": "animation.vex.idle1",
        "idle2": "animation.vex.idle2",
        "idle3": "animation.vex.idle3",
        "move_upper_body": "animation.vex.move_upper_body",
        "move_upper_body_target1": "animation.vex.move_upper_body_target1",
        "move_upper_body_target2": "animation.vex.move_upper_body_target2",
        "move_upper_body_target3": "animation.vex.move_upper_body_target3",
        "move_upper_body_target1_default": "animation.vex.move_upper_body_target1_default",
        "move_upper_body_target2_default": "animation.vex.move_upper_body_target2_default",
        "move_upper_body_target3_default": "animation.vex.move_upper_body_target3_default",
        "move_lower_body": "animation.vex.move_lower_body",
        "attack1": "animation.vex.attack1",
        "attack2": "animation.vex.attack2",
        "attack3": "animation.vex.attack3",
        "dead": "animation.custom_death",
        "dead1": "animation.vex.dead1",
        "dead2": "animation.vex.dead2"
      },
      "particle_effects": {
        "flame": "coptaine:vex_flame",
        "explode": "coptaine:vex_explode"
      },
      "render_controllers": [ "controller.render.vex" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 41
      }
    }
  }
}