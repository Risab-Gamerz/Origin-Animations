{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:zombie",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "zombie",
        "hp": "entity_emissive_alpha"
      },
      "geometry": {
        "default": "geometry.zombie.v1.8",
        "dead": "geometry.zombie.dead.v1.8"
      },
      "textures": {
        "default": "textures/entity/zombie/zombie"
      },
      "scripts": {
        "scale": "q.life_time < 0.2 ? q.life_time * 5 : 1",
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 25.44666) * query.modified_move_speed) * 57.3;",
          "variable.tcos1 = (Math.cos(query.modified_distance_moved * 50.89333) * query.modified_move_speed) * 57.3;"
        ],
        "animate": [
          "root",
          { "look_at_target": "query.is_alive" },
          { "humanoid_baby_big_head": "query.is_baby" },
          { "dead": "!query.is_alive" }
        ],
        "initialize": [
          "v.roll = math.random_integer(1, 5);",
          "v.attack_roll = math.random_integer(1, 3);"
        ]
      },
      "animations": {
        "root": "controller.animation.zombie.root",
        "look_at_target": "animation.common.look_at_target",
        "humanoid_baby_big_head": "controller.animation.humanoid.baby_big_head",
        "humanoid_big_head": "animation.humanoid.big_head",
        "base_pose": "animation.zombie.base_pose",
        "riding.legs": "animation.humanoid.riding.legs",
        "idle1": "animation.zombie.idle1",
        "idle2": "animation.zombie.idle2",
        "idle3": "animation.zombie.idle3",
        "move_upper_body": "animation.zombie.move_upper_body",
        "move_upper_body_target1": "animation.zombie.move_upper_body_target1",
        "move_upper_body_target1_equipped": "animation.zombie.move_upper_body_target1_equipped",
        "move_upper_body_target2": "animation.zombie.move_upper_body_target2",
        "move_upper_body_target2_equipped": "animation.zombie.move_upper_body_target2_equipped",
        "move_upper_body_target3": "animation.zombie.move_upper_body_target3",
        "move_upper_body_target3_equipped": "animation.zombie.move_upper_body_target3_equipped",
        "move_upper_body_target1_default": "animation.zombie.move_upper_body_target1_default",
        "move_upper_body_target1_equipped_default": "animation.zombie.move_upper_body_target1_equipped_default",
        "move_upper_body_target2_default": "animation.zombie.move_upper_body_target2_default",
        "move_upper_body_target2_equipped_default": "animation.zombie.move_upper_body_target2_equipped_default",
        "move_upper_body_target3_default": "animation.zombie.move_upper_body_target3_default",
        "move_upper_body_target3_equipped_default": "animation.zombie.move_upper_body_target3_equipped_default",
        "move_lower_body": "animation.zombie.move_lower_body",
        "attack1": "animation.zombie.attack1",
        "attack1_equipped": "animation.zombie.attack1_equipped",
        "attack2": "animation.zombie.attack2",
        "attack2_equipped": "animation.zombie.attack2_equipped",
        "attack3": "animation.zombie.attack3",
        "attack3_equipped": "animation.zombie.attack3_equipped",
        "dead": "animation.custom_death",
        "dead1": "animation.zombie.dead1",
        "dead2": "animation.zombie.dead2",
        "dead3": "animation.zombie.dead3",
        "dead4": "animation.zombie.dead4",
        "dead5": "animation.zombie.dead5"
      },
      "render_controllers": [
        "controller.render.zombie"
      ],
      "enable_attachables": true,
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 12
      }
    }
  }
}